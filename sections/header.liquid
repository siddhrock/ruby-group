{%- capture image_size -%}{{ section.settings.logo_width | times: 2 }}x{%- endcapture -%}
{% style %}
  .store-logo--image{
    max-width: {{ section.settings.logo_width }}px;
    width: {{ section.settings.logo_width }}px;
  }

  @media only screen and (max-width: 939px) {
    .store-logo--image {
      width: auto;
    }
  }

  {% if section.settings.desktop_layout == 'center' %}
    .utils__item--search-icon,
    .settings-open-bar--small,
    .utils__right {
      width: calc(50% - ({{ section.settings.logo_width }}px / 2));
    }
  {% endif %}
{% endstyle %}

{%- capture logo_html -%}
  <!-- LOGO / STORE NAME -->
  {%- if template.name == 'index' -%}
    <h1 class="h1 store-logo {% if section.settings.desktop_layout == 'center' %} hide-for-search{% endif %} {% if section.settings.logo_position_mobile == 'inline' %} hide-for-search-mobile{% endif %} store-logo--{% if section.settings.logo != blank %}image {% else %}text {% endif %} store-logo-desktop--{{ section.settings.desktop_layout }} store-logo-mobile--{{ section.settings.logo_position_mobile }}" itemscope itemtype="http://schema.org/Organization">
  {%- else -%}
    <div class="h1 store-logo {% if section.settings.desktop_layout == 'center' %} hide-for-search{% endif %} {% if section.settings.logo_position_mobile == 'inline' %} hide-for-search-mobile{% endif %}   store-logo--{% if section.settings.logo != blank %}image{% else %}text{% endif %} store-logo-desktop--{{ section.settings.desktop_layout }} store-logo-mobile--{{ section.settings.logo_position_mobile }}" itemscope itemtype="http://schema.org/Organization">
  {%- endif -%}

  <!-- LOGO -->
  <a href="{{ routes.root_url }}" itemprop="url" class="site-logo{% if section.settings.logo != blank %} site-header__logo-image{% endif %}">
    <meta itemprop="name" content="{{ shop.name }}">
    {%- if section.settings.logo != blank -%}
      {%- capture img_size -%}{{ section.settings.logo_width | times: 2 }}x{%- endcapture -%}
      <img src="{{ section.settings.logo | img_url: img_size }}" alt="{{ section.settings.logo.alt | escape }}" itemprop="logo" width="{{ section.settings.logo_width | times: 2 }}" height="{{ section.settings.logo_width | times: 2 | divided_by: section.settings.logo.aspect_ratio | round }}">
    {%- else -%}
      {{ shop.name }}
    {%- endif -%}
  </a>

  {%- if template.name == 'index' -%}
    </h1>
  {%- else -%}
    </div>
  {%- endif -%}
{%- endcapture -%}

{%- capture desktop_search_icon_html -%}
  {% unless section.settings.show_search_bar_desktop %}
  <!-- DESKTOP SEARCH ICON-->
    <div class="utils__item utils__item--search-icon utils__item--search-{{ section.settings.desktop_layout }} hide-for-search desktop-only">
      <a href="{{ routes.search_url }}" class="plain-link js-search-form-open" aria-label="{{ 'general.accessibility_labels.search' | t | escape }}">
        <span class="icon--header">
          {% render 'svg-search' %}
        </span>
        <span class="icon-fallback-text">{{ 'general.search.submit' | t }}</span>
      </a>
    </div>
  {% endunless %}
{%- endcapture -%}

{%- capture mobile_search_icon_html -%}
  <!-- MOBILE SEARCH ICON-->
  <a href="{{ routes.search_url }}" class="plain-link utils__item utils__item--search-icon utils__item--search-{{ section.settings.logo_position_mobile }} hide-for-search mobile-only js-search-form-open" aria-label="{{ 'general.accessibility_labels.search' | t | escape }}">
    <span class="icon--header">
      {% render 'svg-search' %}
    </span>
    <span class="icon-fallback-text">{{ 'general.search.submit' | t }}</span>
  </a>
{%- endcapture -%}

{%- capture desktop_search_html -%}
<!-- DESKTOP SEARCH -->
  {% assign search_id = 'open-search-closed '%}
  {% if section.settings.show_search_bar_desktop %}
    {% capture search_bar_class %}settings-open-bar desktop-only {% if section.settings.desktop_layout == 'center' %} settings-open-bar--small{% endif%}{% endcapture %}
    {% assign search_id = 'open-search-bar '%}
  {% else %}
    {% capture search_bar_class %}{% if section.settings.desktop_layout == 'left' %}settings-close-bar--left{% endif%} desktop-only{% endcapture %}
  {% endif %}

  {% render 'search-bar', settings_class: search_bar_class, search_id: search_id %}
{%- endcapture -%}

{%- capture mobile_search_html -%}
  <!-- MOBILE SEARCH -->
  {% assign search_id = 'open-search-mobile-closed '%}
  {% if section.settings.show_search_bar_mobile %}
    {% capture search_bar_class %} settings-open-mobile-bar{% endcapture %}
    {% assign search_id = 'open-search-mobile-bar '%}
  {% else %}
    {% capture search_bar_class %} settings-close-mobile-bar{% endcapture %}
  {% endif %}

  {% render 'search-bar-mobile', settings_class: search_bar_class, search_id: search_id %}
{%- endcapture -%}

{%- capture account_html -%}
  <div class="utils__item customer-account hide-for-search">
    <a href="{{ routes.account_url }}" class="customer-account__parent-link icon--header" aria-haspopup="true" aria-expanded="false" aria-label="{{ 'general.accessibility_labels.account' | t | escape }}">
      {% render 'icon' with 'user' %} <div class="sm_hide header-icon__text">Sign in</div>
    </a>
    <ul class="customer-account__menu">
      {% if customer %}
        <li class="customer-account__link">
          {% if customer.first_name != blank %}
            {% capture first_name %}<a href="{{ routes.account_url }}">{{ customer.first_name }}</a>{% endcapture %}
            {{ 'layout.customer.logged_in_as_html' | t: first_name: first_name }}
          {% endif %}
        </li>
        <li class="customer-account__link">
          <a href="{{ routes.account_url }}">{{ 'layout.customer.account' | t }}</a>
        </li>
        <li class="customer-account__link customer-logout">
          <a href="{{ routes.account_logout_url }}">
            {{ 'layout.customer.log_out' | t }}
          </a>
        </li>
      {% else %}
        <li class="customer-account__link customer-login">
          <a href="{{ routes.account_login_url }}">
            {{ 'layout.customer.log_in' | t }}
          </a>
        </li>
        <li class="customer-account__link customer-register">
          <a href="{{ routes.account_register_url }}">
          {{ 'layout.customer.create_account' | t }}
          </a>
        </li>
        <li class="customer-account__link customer-register">
          <a href="/pages/trade-application">
            Apply for Trade Account
          </a>
        </li>
      {% endif %}
    </ul>
  </div>
{%- endcapture -%}

{%- if template == 'gift_card' -%}

  <div class="site-header site-header--logo-only">
    <header class="header-content container">
      <div class="page-header page-width">
        <div class="h1 store-logo store-logo--{% if section.settings.logo != blank %}image{% else %}text{% endif %}"
          itemscope itemtype="http://schema.org/Organization">
          <meta itemprop="url" content="{{ shop.url }}">
          <meta itemprop="name" content="{{ shop.name }}">
          {% if section.settings.logo != blank %}
            <h1 class="site-logo site-header__logo-image">
              {% capture img_size %}{{ section.settings.logo_width | times: 2 }}x{% endcapture %}
              <img src="{{ section.settings.logo | img_url: img_size }}" alt="{{ section.settings.logo.alt | escape }}" itemprop="logo" width="{{ section.settings.logo.width }}" height="{{ section.settings.logo.height }}">
            </h1>
          {% else %}
            <h1 class="site-logo">{{ shop.name }}</h1>
          {% endif %}
        </div>
      </div>
    </header>
  </div>

{%- else -%}

  <div class="site-header {% if section.settings.dock_nav %}docking-header{% endif %}" data-section-id="{{ section.id }}" data-section-type="header" role="banner">
    <header class="header-content container">
      <div class="custom_nav">
        <ul>
          {% for link in linklists[section.settings.announce_nav].links %}
            <li><a href="{{ link.url }}">{{ link.title }}</a></li>
          {% endfor %}
        </ul>
      </div>
      <div class="page-header page-width">
        
        {%- if section.settings.desktop_layout == 'left' or section.settings.logo_position_mobile == 'own_row' -%}
          {{ logo_html }}
        {%- endif -%}

        {%- if section.settings.dock_nav -%}
        <div class="docked-mobile-navigation-container">
        <div class="docked-mobile-navigation-container__inner">
        {%- endif -%}

        <div class="utils relative {% if section.settings.desktop_layout == 'center' %} utils--center{% elsif section.settings.desktop_layout == 'left' %} custom_left_style{% endif %}">
          <!-- MOBILE BURGER -->
         

          {%- if section.settings.desktop_layout == 'center' -%}
            {{ desktop_search_icon_html }}
            {{ desktop_search_html }}
          {%- endif -%}

          {%- if section.settings.desktop_layout == 'center' or section.settings.logo_position_mobile == 'inline' -%}
            {{ logo_html }}
          {%- endif -%}

          <div class="utils__right">
            {% comment %}
            {%- if section.settings.show_country_selector or section.settings.show_language_selector -%}
              <!-- CURRENCY CONVERTER -->
              <div class="utils__item utils__item--currency header-localization header-localization--desktop hide-for-search">
                {% render 'localization', input_prefix: 'desktop-header' %}
              </div>
            {%- endif -%}
            {% endcomment %}

            {%- if section.settings.desktop_layout == 'left' and section.settings.show_search_bar_desktop == false -%}
              {{ desktop_search_icon_html }}
              {{ desktop_search_html }}
            {%- endif -%}

            {%- if section.settings.show_search_bar_mobile == false -%}
              {{ mobile_search_icon_html }}
              {{ mobile_search_html }}
            {%- endif -%}
            
            {%- if section.settings.desktop_layout == 'left' and section.settings.show_search_bar_desktop == true -%}
              {{ desktop_search_html }}
            {%- endif -%}

           {% if section.settings.contact_1_title != blank or section.settings.contact_2_title != blank %}
             <div class="contact-blocks-wrap">
               {% if section.settings.contact_info_title != blank %}
                <h3 class="contact-blocks__title">{{ section.settings.contact_info_title }}</h3>
              {% endif %}
               <div class="contact-blocks">
              {% if section.settings.contact_1_title %}
                <a href="{{ section.settings.contact_1_link }}" class="contact-block contact-block--header" target="_blank">
                  <div class="contact-block__icon-text">
                    <span class="contact-block__icon">
                     {{ section.settings.contact_1_icon }}
                    </span>
                    <span class="contact-block__title">
                      {{ section.settings.contact_1_title }}
                    </span>
                  </div>
                  {% if section.settings.contact_1_text != blank %}
                    <p class="contact-block__text">
                      {{ section.settings.contact_1_text }}
                    </p>
                  {% endif %}
                  <p 
                    class="contact-block__availability"
                    data-available-text="{{ section.settings.contact_1_availability_text }}"
                    data-unavailable-text="{{ section.settings.contact_1_unavailability_text }}"
                    >
                    <span class="icon_phone_dot"> </span>
                    <span class="contact-block__avail_text">{{ section.settings.contact_1_availability_text }}</span>
                  </p>
                </a>
              {% endif %}
  
              {% if section.settings.contact_2_title %}
                <div class="contact-block contact-block--header" target="_blank">
                  <a href="{{ section.settings.contact_2_link }}" {% if section.settings.contact_2_link != "#" %} target="_blank" {% endif %}>
                    <div class="contact-block__icon-text">
                      <span class="contact-block__icon">
                        {{ section.settings.contact_2_icon }}
                      </span>
                      <span class="contact-block__title">
                        {{ section.settings.contact_2_title }}
                      </span>
                    </div>
                    {% if section.settings.contact_2_text != blank %}
                      <p class="contact-block__text">
                        {{ section.settings.contact_2_text }}
                      </p>
                    {% endif %}
                    <p 
                      class="contact-block__availability"                  
                      data-available-text="{{ section.settings.contact_2_availability_text }}"
                      data-unavailable-text="{{ section.settings.contact_2_unavailability_text }}"
                      >
                       <span class="icon_phone_dot"></span>
                      <span class="contact-block__avail_text">{{ section.settings.contact_2_availability_text }}</span>
                    </p>
                  </a>

                  <div class="popover">
                    <div class="popover__panel">
                      <h4 class="popover__title">
                        {{ section.settings.popover_title }}
                      </h4>
                      <div class="popover__panel-list">
                        <a href="{{ section.settings.popover_item_1_link }}" class="popupover__panel-item" target="_blank">
                          <div class="icon-text">
                            <span class="icon">{{ section.settings.popover_item_1_svg  }}</span>
                            <div class="icon-text__details">
                              <span class="icon-text__title">{{ section.settings.popover_item_1_title }}</span>
                              <p class="contact-block__availability">
                                <span class="icon_phone_dot"></span>
                                <span class="icon-text__text">{{ section.settings.popover_item_1_text }}</span>
                              </p>
                            </div>
                          </div>
                          <div class="popupover__panel-schedule">
                            {{ section.settings.popover_item_1_schedule }}
                          </div>
                        </a>
                        <a href="{{ section.settings.popover_item_2_link }}" class="popupover__panel-item" target="_blank">
                          <div class="icon-text">
                            <span class="icon">{{ section.settings.popover_item_2_svg  }}</span>
                            <div class="icon-text__details">
                              <span class="icon-text__title">{{ section.settings.popover_item_2_title }}</span>
                              <span class="icon-text__text">{{ section.settings.popover_item_2_text }}</span>
                            </div>
                          </div>
                          <div class="popupover__panel-schedule">
                            {{ section.settings.popover_item_2_schedule }}
                          </div>
                        </a>
                        <a href="{{ section.settings.popover_item_3_link }}" class="popupover__panel-item" target="_blank">
                          <div class="icon-text">
                            <span class="icon">{{ section.settings.popover_item_3_svg  }}</span>
                            <div class="icon-text__details">
                              <span class="icon-text__title">{{ section.settings.popover_item_3_title }}</span>
                              <span class="icon-text__text">{{ section.settings.popover_item_3_text }}</span>
                            </div>
                          </div>
                          <div class="popupover__panel-schedule">
                            {{ section.settings.popover_item_3_schedule }}
                          </div>
                        </a>
                        <a href="{{ section.settings.popover_item_4_link }}" class="popupover__panel-item" target="_blank">
                          <div class="icon-text">
                            <span class="icon">{{ section.settings.popover_item_4_svg  }}</span>
                            <div class="icon-text__details">
                              <span class="icon-text__title">{{ section.settings.popover_item_4_title }}</span>
                              <p class="contact-block__availability">
                                <span class="icon_phone_dot"></span>
                                <span class="icon-text__text">{{ section.settings.popover_item_4_text }}</span>
                              </p>
                            </div>
                          </div>
                          <div class="popupover__panel-schedule">
                            {{ section.settings.popover_item_4_schedule }}
                          </div>
                        </a>
                      </div>
                    </div>
                  </div>
                </div>
              {% endif %}
            </div>
           </div>
            {% endif %}
            
            <!-- Trade Application -->
            {% comment %}
            <a href="/pages/trade-application" class="icon_trade">
               {% render 'icon_trade' %} 
                <div class="sm_hide">Trade Application</div>
            </a>
            {% endcomment %}
            <!-- Mobile telephone icon -->
            <div class="header-utils__actions">
            {% comment %}
            {% if section.settings.tel_num != blank %}
              <a href="tel:{{ section.settings.tel_num }}" class="customer-account utils__item icon_phone_mob">
                <span class="icon--header main_number">{% render 'telephone_icon' %}</span>
              </a>
            {% endif %}
            {% endcomment %}
            
            <!-- ACCOUNT -->
            {% if shop.customer_accounts_enabled %}
              {{ account_html }}
            {% endif %}

            <!-- CART -->
            {% capture cart_html %}
            <div class="utils__item header-cart hide-for-search">
              <a href="{{ routes.cart_url }}">
                <div class="cart_icon_main">
                  <span class="icon--header">{% render 'icon' with section.settings.cart_icon %}</span>
                  {% assign extra_pro = 0 %}
                  {% for sub_item in cart.items %}
                    {% if sub_item.product.tags contains 'hidden' %}
                      {% assign extra_pro = extra_pro | plus: sub_item.quantity %}
                    {% endif %}
                    {% endfor %}
                  <span class="header-cart__count">{{ cart.item_count | minus: extra_pro }}</span>
                </div>
                <span class="sm_hide header-icon__text">Cart</span>
              </a>
              {% unless cart.items == empty or template contains 'cart' %}
                {% if settings.on_add_to_cart == 'ajax' %}
                <div class="cart-summary global-border-radius">
                  <div class="cart-summary__header">{{ 'layout.cart.title' | t }}:</div>
                  <div class="cart-summary__product-list">
                    {% for item in cart.items %}
                      {%- unless item.product.tags contains 'hidden' -%}
                        <div class="cart-summary__product">
                          <div class="cart-summary__product-image">
                            {% render 'responsive-image', image: item.image %}
                          </div>
                          <div class="cart-summary__product__description">
                            <div class="cart-summary__product-title">
                              {{ item.product.title | link_to: item.product.url }}
                              {% if item.quantity > 1 %}
                                <span class="cart-summary__quantity">{{ item.quantity }}</span>
                              {% endif %}
                            </div>
                            <div class="cart-summary__product__variants">
                              {% for option in item.product.options %}
                                {% capture value_key %}option{{ forloop.index }}{% endcapture %}
                                {% if item.variant[value_key] != 'Default Title' %}
                                  <div class="added-notice__variant">
                                    <span class="added-notice__variant-label">{{ option }}:</span>
                                    <span class="added-notice__variant-value">{{ item.variant[value_key] }}</span>
                                  </div>
                                {% endif %}
                              {% endfor %}
  
                            {% render 'line-item-properties', line_item: item %}
  
                            </div>
                            {% if item.product.template_suffix contains 'preorder' %}
                              <div class="cart-summary__preorder">{{ 'products.product.preorder' | t }}</div>
                            {% endif %}
                            {% if item.selling_plan_allocation %}
                              <div class="cart-summary__selling-plan small-text">{{ item.selling_plan_allocation.selling_plan.name }}</div>
                            {% endif %}
                            <div class="cart-summary__price">
                              {% if item.original_line_price != item.final_line_price %}
                              <span class="product-price__reduced theme-money">{{ item.final_line_price | money }}</span>
                                <span class="product-price__compare theme-money">{{ item.original_line_price | money }}</span>
                              {% else %}
                                <span class="theme-money">{{ item.final_line_price | money }}</span>
                              {% endif %}
                              {%- if item.unit_price_measurement -%}
                                <div class="cart-summary__unit-price">
                                  {% render 'unit-price', variant: item %}
                                </div>
                              {%- endif -%}
  
                              {% if item.line_level_discount_allocations.size > 0 %}
                                <ul class="cart-discount-list small-text">
                                {% for discount_allocation in item.line_level_discount_allocations %}
                                  <li class="cart-discount cart-discount--inline">
                                    <span class="cart-discount__label">{{ discount_allocation.discount_application.title }}</span>
                                    <span class="cart-discount__amount theme-money">{{ discount_allocation.amount | money }}</span>
                                  </li>
                                {% endfor %}
                                </ul>
                              {% endif %}
                            </div>
                          </div>
                        </div>
                      {% endunless %}
                    {% endfor %}
                  </div>
                  {% assign extra_pro = 0 %}
                  {% for sub_item in cart.items %}
                    {% if sub_item.product.tags contains 'hidden' %}
                      {% assign extra_pro = extra_pro | plus: sub_item.quantity %}
                    {% endif %}
                  {% endfor %}
                  <div class="cart-summary__footer">
                    <div class="cart-summary__total-quantity-row">
                      <span>{{ 'layout.cart.items' | t }}</span>
                      <span>{{ cart.item_count | minus: extra_pro }}</span>
                    </div>

                    {%- if cart.cart_level_discount_applications != blank -%}
                      <ul class="cart-discount-list">
                        {%- for discount_application in cart.cart_level_discount_applications -%}
                          <li class="cart-discount cart-discount--inline">
                            <span class="cart-discount__label">{{ discount_application.title }}</span>
                            <span class="cart-discount__amount theme-money">{{ discount_application.total_allocated_amount | money }}</span>
                          </li>
                        {%- endfor -%}
                      </ul>
                    {%- endif -%}
                   {% assign extra_cost = 0 %}
                    {% for sub_item in cart.items %}
                      {% if sub_item.product.tags contains 'hidden' %}
                        {% assign extra_cost = extra_cost | plus: sub_item.final_line_price %}
                      {% endif %}
                    {% endfor %}
                    {% if extra_cost != 0  %}
                      <div class="mini-cart__recap-price-line-extra">
                        <span>{{ 'layout.cart.extra' | t }}</span>
                        <span>
                          {%- if settings.cart_currency_code_enabled -%}
                            {{- extra_cost | money_with_currency -}}
                          {%- else -%}
                            {{ extra_cost | money }}
                          {%- endif -%}
                        </span>
                      </div>
                    {% endif %}
                    <div class="cart-summary__total-price-row large-text">
                      <span>{{ 'layout.cart.subtotal' | t }}</span>
                      <span class="theme-money">
                        {%- if settings.cart_currency_code_enabled -%}
                          {{- cart.total_price | money_with_currency -}}
                        {%- else -%}
                          {{- cart.total_price | money -}}
                        {%- endif -%}
                      </span>
                    </div>

                    <a class="btn btn--primary btn--small btn--fullwidth cart-summary__button" href="{{ routes.cart_url }}">{{ 'layout.cart.go_to_cart' | t }}</a>
                  </div>
                </div>
                {% endif %}
              {% endunless %}
            </div>
            {% endcapture %}
            {{ cart_html }}
              
             <button class="btn btn--plain burger-icon js-mobile-menu-icon hide-for-search" aria-label="{{ 'general.accessibility_labels.menu_toggle' | t }}">
            {% render 'icon' with 'menu' %}
          </button>
            </div>
          </div>

        </div>

        {% if section.settings.dock_nav %}
        </div>
        </div>
        {% endif %}
      </div>
    </header>

    {% if section.settings.contact_1_title != blank or section.settings.contact_2_title != blank %}
       <div class="contact-blocks-wrap contact-blocks-wrap--mobile js-open-contact-modal">
         <div class="contact-blocks">
        {% if section.settings.contact_1_title != blank %}
          <div class="contact-block contact-block--header">
            <div class="contact-block__icon-text">
              <span class="contact-block__icon">
                {{ section.settings.contact_1_icon }}
              </span>
              <div class="contact-block__details">
                <span class="contact-block__title">
                  {{ section.settings.contact_1_title }}
                </span>
                <p 
                  class="contact-block__availability"
                  data-available-text="{{ section.settings.contact_1_availability_text }}"
                  data-unavailable-text="{{ section.settings.contact_1_unavailability_text }}"
                >
                  <span class="icon_phone_dot"></span>
                  <span class="contact-block__avail_text">{{ section.settings.contact_1_availability_text }}</span>
                </p>
              </div>
            </div>
          </div>
        {% endif %}

        {% if section.settings.contact_2_title %}
          <div class="contact-block contact-block--header">
            <div class="contact-block__icon-text">
              <span class="contact-block__icon">
                {{ section.settings.contact_2_icon }}
              </span>
              <div class="contact-block__details">
                <span class="contact-block__title">
                  {{ section.settings.contact_2_title }}
                </span>
                <p 
                  class="contact-block__availability"
                  data-available-text="{{ section.settings.contact_2_availability_text }}"
                  data-unavailable-text="{{ section.settings.contact_2_unavailability_text }}"
                >
                   <span class="icon_phone_dot"></span>
                   <span class="contact-block__avail_text">{{ section.settings.contact_2_availability_text }}</span>
                </p>
              </div>
            </div>
          </div>
        {% endif %}
      </div>
     </div>
    {% endif %}

    {% if section.settings.dock_nav %}
    <div class="docked-navigation-container docked-navigation-container--{{ section.settings.desktop_layout }}">
    <div class="docked-navigation-container__inner">
    {% endif %}

    <div class="docked-navigation__search-box container">
      <div class="docked-navigation__search">
        {% render 'search-bar', settings_class: search_bar_class, search_id: search_id, placeholder: "Search our store..." %}
      </div>
